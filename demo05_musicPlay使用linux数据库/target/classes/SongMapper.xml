<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--
    namespace:名称空间
-->

<mapper namespace="SongMapper">

    <!--statement-->
    <select id="selectAll">
        select * from songs_info LIMIT 200;
    </select>
	<select id="search">
        select * from songs_info where singerName=#{singer};
    </select>





</mapper>